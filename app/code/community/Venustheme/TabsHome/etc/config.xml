<?xml version="1.0"?>  
<config>  
    <modules>  
        <Venustheme_TabsHome>  
            <version>1.1.0</version>  
        </Venustheme_TabsHome>  
    </modules>  
    <!-- frontend -->
    <frontend>
        <translate>
            <modules>
                <Venustheme_TabsHome>
                    <files>
                        <default>Venustheme_TabsHome.csv</default>
                    </files>
                </Venustheme_TabsHome>
            </modules>
        </translate>
        <layout>
            <updates>
                <tabshome>
                    <file>tabshome.xml</file>
                </tabshome>
            </updates>
        </layout>
        <routers>
            <tabshome>
                <use>standard</use>
                <args>
                    <module>Venustheme_TabsHome</module>
                    <frontName>tabshome</frontName>
                </args>
            </tabshome>
        </routers>
    </frontend>
    <!--Global-->
    <global>  
        
        <blocks>
            <venustheme_tabshome>
                <class>Venustheme_TabsHome_Block</class>
            </venustheme_tabshome>
        </blocks>
        <models>
            <venustheme_tabshome>
                <class>Venustheme_TabsHome_Model</class>
            </venustheme_tabshome>
        </models>
        <helpers>
            <venustheme_tabshome>
                <class>Venustheme_TabsHome_Helper</class>
            </venustheme_tabshome>
        </helpers>
        <events>
            <controller_action_layout_generate_blocks_after>
                <observers>
                    <venustheme_tabshome_observer>
                        <type>singleton</type>
                        <class>Venustheme_TabsHome_Model_Observer</class>
                        <method>beforeRender</method>
                    </venustheme_tabshome_observer>
                </observers>
            </controller_action_layout_generate_blocks_after>
        </events>
    </global>
    <admin>
        <routers>
            <tabshome>
                <use>admin</use>
                <args>
                    <module>Venustheme_TabsHome</module>
                    <frontName>tabshome</frontName>
                </args>
            </tabshome>
        </routers>
    </admin>
	 <adminhtml>
        <acl>
            <resources>
                <admin>
                    <children>
						<venustheme_tabshome>
                            <title>File</title>
                            <sort_order>50</sort_order>
                            <children>
                                <index>
                                    <title>Manage Files</title>
                                    <sort_order>0</sort_order>
                                </index>
                                <new>
                                    <title>Add File</title>
                                    <sort_order>1</sort_order>
                                </new>
                            </children>
                        </venustheme_tabshome>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <venustheme_tabshome>
                                            <title>Ves TabsHome</title>
                                        </venustheme_tabshome>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
		<menu>
            <vesextensions module="venustheme_tabshome">
                <title>VenusTheme</title>
                <sort_order>53</sort_order>
                <children>
					<tabshome translate="title" module="venustheme_tabshome">
						<title>Ves TabsHome</title>
						<children>
							
							<configuration translate="title" module="venustheme_tabshome">
								<title>Configuration</title>
								<sort_order>4</sort_order>
								<action>adminhtml/system_config/edit/section/venustheme_tabshome_info</action>
							</configuration>
						</children>
					</tabshome>
                </children>
            </vesextensions>
        </menu>
        <translate>
			<modules>
				<Venustheme_TabsHome>
					<files>
						<default>Venustheme_TabsHome.csv</default>
					</files>
				</Venustheme_TabsHome>
			</modules>
		</translate>
    </adminhtml>	
    <default>
        <venustheme_tabshome_info>
            <venustheme_tabshome>
                <theme>default</theme>
                <show>1</show>
                
				<enable_jquery>0</enable_jquery>
                
                <enable_feature>1</enable_feature>
                <enable_bestseller>0</enable_bestseller>
                <enable_mostview>1</enable_mostview>
                <enable_new>1</enable_new>
                <enable_special>0</enable_special>
                <limit_item>6</limit_item>
                <set_time>2</set_time>
                <module_width>auto</module_width>
                <blockPosition>content</blockPosition>
				<blockDisplay>before</blockDisplay>
				<menuAssignment>Mage_Cms_index</menuAssignment>
                <slider_speed>800</slider_speed>
                <auto_play>0</auto_play>
                <blockDisplay>before</blockDisplay>
				<itemsrow>4</itemsrow>
            </venustheme_tabshome>
            <main_slider>
                <open_link>_blank</open_link>
                <max_char>100</max_char>
                <cre_main_size>0</cre_main_size>
                <tooltip_style>vs-tooltip</tooltip_style>
                <main_imgsize>home</main_imgsize>
                <limit_col>4</limit_col>
                <limit_row>2</limit_row>
                
                <main_height>147</main_height>
                <main_width>131</main_width>
                
                <show_des>1</show_des>
                <show_price>1</show_price>
                <show_price_without>1</show_price_without>
                <show_title>1</show_title>
                <show_img>1</show_img>
                <show_public_date>1</show_public_date>
                <show_btn>1</show_btn>
                <online_icon>1</online_icon>
                <feature_icon>1</feature_icon>
                <new_icon>1</new_icon>
                <sale_icon>1</sale_icon>
            </main_slider>
        </venustheme_tabshome_info>
    </default>
</config>  